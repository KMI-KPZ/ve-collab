{
    "extends": [
        "eslint:recommended",
        "next/core-web-vitals",
        "plugin:react/recommended",
        "prettier"
    ],
    "globals": {
        "JSX": true
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "no-unused-vars": "off",
        "react/no-unknown-property": "off"
    }
}
